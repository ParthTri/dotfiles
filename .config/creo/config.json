{
	"Python": {
		"inherit": false,
		"git": true,
		"gitignore": "__pycache__\n.venv/",
		"env": false,
		"externalProgramsEnd": [
			"python3 -m venv .venv"
		],
		"dirs": [
			"tests",
			"src",
			"docs",
			"docs/examples"
		],
		"files": [
			"docs/Purpose.org",
			"README.org"
		]
	},
	"Solid": {
		"inherit": false,
		"ignoreProjectPrexists": true,
		"git": false,
		"env": false,
		"externalProgramsStart": [
			"pnpx degit solidjs/templates/js $name"
		],
		"externalProgramsEnd": [
			"pnpm install"
		]
	},
	"Go": {
		"inherit": false,
		"ignoreProjectPrexists": false,
		"git": true,
		"env": false,
		"dirs": [
			"cmd",
			"lib"
		],
		"files": [
			"cmd/main.go"
		]
	},
	"Gin": {
		"inherit": true,
		"parent": "Go",
		"ignoreProjectPrexists": true,
		"git": true,
		"env": false,
		"dirs": [
			"routes",
			"db",
			"middleware",
			"server"
		],
		"files": [
			"main.go",
			"db/db.go",
			"routes/routes.go",
			"middleware/middleware.go",
			"server/server.go"
		],
		"externalProgramsEnd": [
			"go mod init $name",
			"go get -u github.com/gin-gonic/gin"
    ]
  },
	"Blank": {
		"inherit": false,
		"git": true,
		"gitignore": "",
		"dirs": [
			"src"
		]
	},
	"Express": {
		"git": true,
		".gitignore": "node_modules\n",
		"dirs": [
			"routes",
			"middleware",
			"models",
			"public",
			"public/images",
			"public/images/stylesheets",
			"public/images/javascript"
		],
		"files": [
			"app.js"
		]
	}
}
  
